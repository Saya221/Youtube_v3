<div class="center">
  <h3><%= "The Lastest Youtube URL was shared by everyone"%></h3>
</div>

<div class="container">
  <% @shared_urls["data"]["shared_urls"].each do |shared_url| %>
    <div>
      <p>
        <%= link_to "#{shared_url['url']}", shared_url["url"] %>
        <%= "was shared by #{shared_url['user']['name']}" %>
      </p>
    </div>
  <% end %>
</div>
